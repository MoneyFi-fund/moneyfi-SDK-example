{
  "name": "moneyfi-sdk",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 vite build && tsc",
    "build:dev": "NODE_ENV=development vite build --mode development --sourcemap && tsc",
    "serve": "vite preview",
    "test": "vitest run",
    "lint": "eslint --fix .",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "generate-routes": "tsr generate",
    "watch-routes": "tsr watch",
    "check-types": "tsc --pretty --noEmit",
    "pre-commit": "pnpm lint && pnpm prettier && pnpm check-types",
    "generate": "orval --config ./orval.config.ts",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "wrangler-deploy": "npx wrangler pages deploy dist"
  },
  "dependencies": {
    "@aptos-labs/ts-sdk": "^4.0.0",
    "@aptos-labs/wallet-adapter-react": "^7.0.4",
    "@chakra-ui/react": "^3.26.0",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-devtools": "^0.2.2",
    "@tanstack/react-query": "^5.87.4",
    "@tanstack/react-query-devtools": "^5.87.4",
    "@tanstack/react-router": "^1.130.2",
    "@tanstack/react-router-devtools": "^1.131.5",
    "@tanstack/router-plugin": "^1.121.2",
    "aptosmoneyfimockupupgrade": "^0.0.1",
    "clsx": "^2.1.1",
    "cross-env": "^10.0.0",
    "lodash": "^4.17.21",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.0.6",
    "moneyfi-ts-sdk": "^0.0.1",
    "vite-plugin-eslint": "^1.8.1"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.12.3",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^8.57.1",
    "@tanstack/eslint-plugin-query": "^5.86.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/lodash": "^4.17.20",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.22.0",
    "eslint-config-react-app": "^7.0.1",
    "jsdom": "^26.0.0",
    "react-icons": "^5.5.0",
    "sass-embedded": "^1.92.1",
    "typescript": "^5.7.2",
    "vite": "^6.3.5",
    "vitest": "^3.0.5",
    "web-vitals": "^4.2.4"
  }
}
